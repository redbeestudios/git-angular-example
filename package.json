{
  "name": "git-angular-example",
  "version": "1.0.0",
  "description": "Angular 1 application to show how it works",
  "main": "app/app.js",
  "scripts": {
    "postinstall": "sh configure.sh"
  },
  "moduleConfig": {
    "mainJs": "app/app.js",
    "source": "app/**/*.js",
    "templates": "app/**/*.html",
    "sass": [
      "styles/**/*.scss",
      "styles/**/*.css"
