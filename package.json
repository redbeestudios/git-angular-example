    "gulp-css-base64": "1.3.2",
    "gulp-img64": "0.0.6",
    "gulp-inject": "3.0.0",
    "gulp-jshint": "1.11.2",
    "gulp-minify-html": "1.0.4",
    "gulp-ng-annotate": "1.1.0",
    "gulp-plumber": "1.0.1",
    "gulp-replace-task": "0.11.0",
